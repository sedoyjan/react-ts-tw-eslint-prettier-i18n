{
  "name": "react-ts-tw-eslint-prettier-i18n",
  "version": "1.0.0",
  "description": "Template for client app with react ts eslint prettier tailwind and i18n",
  "main": "src/index.tsx",
  "repository": "https://github.com/sedoyjan/react-ts-tw-eslint-prettier-i18n.git",
  "author": "Sergei Grigorev <sedoyjan@live.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "dev": "rimraf dist && webpack serve --config config/webpack.dev.js",
    "build": "webpack --config config/webpack.prod.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "yarn lint:js && yarn lint:ts",
    "lint:js": "eslint '**/*.{js,json,ts,tsx}'",
    "lint:js:fix": "eslint '**/*.{js,json,ts,tsx}' --fix",
    "lint:ts": "tsc -p tsconfig.json --noEmit --skipLibCheck",
    "extract": "yarn i18next  'src/**/*.{ts,tsx}' --config i18next-parser.config.js"
  },
  "devDependencies": {
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "autoprefixer": "^10.4.13",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-no-cyrillic-string": "^1.0.5",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "i18next-parser": "^7.6.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.4",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.14",
    "classnames": "^2.3.2",
    "daisyui": "^2.49.0",
    "i18next": "^22.4.9",
    "i18next-browser-languagedetector": "^7.0.1",
    "preact": "^10.11.3",
    "preact-i18next": "^11.8.5",
    "react-i18next": "^12.1.4",
    "tailwind-merge": "^1.8.1"
  }
}
